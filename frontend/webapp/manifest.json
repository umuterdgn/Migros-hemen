{
  "sap.app": {
    "id": "migros",
    "type": "application",
    "title": "Migros Hemen",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "migros.view.Login",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.71",
      "libs": {
        "sap.m": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "migros.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "customer",
          "name": "customer",
          "target": "customer"
        },
        {
          "pattern": "store",
          "name": "store",
          "target": "store"
        }
      ],
      "targets": {
        "login": {
          "viewName": "Login"
        },
        "customer": {
          "viewName": "Customer"
        },
        "store": {
          "viewName": "Store"
        }
      }
    }
  }
}
